{
  "name": "nuxt-lettermint",
  "version": "1.0.2",
  "description": "Nuxt module for sending emails with Lettermint",
  "author": "Bjorn Antonissen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lettermint/nuxt-lettermint.git"
  },
  "bugs": {
    "url": "https://github.com/lettermint/nuxt-lettermint/issues"
  },
  "homepage": "https://github.com/lettermint/nuxt-lettermint#readme",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "email",
    "lettermint",
    "mail",
    "smtp",
    "transactional-email"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "defu": "^6.1.4",
    "lettermint": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@types/node": "^25.0.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.1"
  }
}
